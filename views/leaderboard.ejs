<%- include("partials/Header.ejs")%>
<!--To publish the errors on screen for user if any were found-->

<div class="container" style="min-height:100vh;">
  <%-messages("partials/Messages.ejs", locals)%>

  <section class="profile-section2">

    <div class="accordion" id="accordionExample">
        <div class="competitionExamHeader">
            <h1><%= comp_TITLE %></h1>
        </div>
        <ul class="competiton-breadcrumb">
            <li><a href="/competitions/details/<%= comp_ID %>" >Details</a></li>
            <li><a href="/competitions/reviews/<%= comp_ID %>">Reviews</a></li>
            <li><a href="/competitions/leaderboard/<%= comp_ID %>/<%= comp_TITLE %>" class="active">Leaderboard</a></li>
        </ul>
        <div class="table-responsive">
            <table class="table table-striped table-sm">
              <thead>
                <tr>
                  <th>Rank</th>
                  <th>Username</th>
                  <th>Grade</th>
                  <th>Duration</th>
                  <th>Score</th>
                </tr>
              </thead>
              <tbody>
                  <%for(var i=0;i<List.length;i++){%>
                <tr>
                  <td><%=i+1%></td>
                  <td><%=List[i].Username%></td>
                  <td><%=List[i].grade%></td>
                  <td><%=List[i].duration%></td>
                  <td><%=List[i].score%></td>
                </tr>
                <%}%>
              </tbody>
            </table>
          </div>
    </div>
    <a href="/competitions/reviews/<%=comp_ID%>" role="button" class="btn btn-outline-Custom" style="margin-top: 10%;">Leave A Review</a>
  </section>
</div>

<%- include("partials/Footer.ejs")%>